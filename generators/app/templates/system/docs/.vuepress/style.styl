// Fixing the Style bleeds
.demo-block, .el-picker-panel

  table
    border-collapse inherit
    margin 0

  tr
    border-top none
    &:nth-child(2n)
      background-color white

  th, td
    border none
    background-color white
    padding inherit

.video-container 
  position: relative;
  overflow: hidden;
  padding-top: 56.25%;

  iframe
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    border: 0;